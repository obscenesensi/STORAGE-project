#include <iostream>
#include <string>
using namespace std;

int main()
{
  int t;
	cin >> t;
	for (int i =0; i< t; i++)
	{
		int x,y,z;
		cin >> x >> y >> z;
		string str;
		cout << "Case "<< i+1 << ": ";
		if( x==y && x == z)
		    str = "Both\n";
		else if (x+z == 2*y)
		    str = "Arithmetic Progression\n";
		else if (x*z == y*y)
		    str = "Geometric Progression\n";
		else str = "None\n";
		cout << str ;
	}
	return 0;
}
