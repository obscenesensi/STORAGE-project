#include <stdio.h>
#define CM_TO_INCH(x) (x * 2.75) //inch로 바꿔주는 메크로
 
int main(void)
{
           int num;
           double result;// 결과값을 넣을 변수
 
           printf("inch로 변환할 cm를 입력하시오\n>> ");
           scanf("%d",&num); //수를 입력받는 부분
 
           result = CM_TO_INCH(num);
 
           printf("%dcm는 %.2lfinch입니다.\n",num,result);
 
           return 0;
} 
