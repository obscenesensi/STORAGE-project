#include <stdio.h>
#define LEN 10
int main()
{    
    int s[LEN] = { 22, 10, 54, 25, 1, 5, 7, 34, 2, 3};
    
    int i,j,temp;
    
    for(i=1;i<LEN;i++)
    {
        temp = s[i];
        j = i-1;
        while(j>=0 && s[j] > temp)
        {
            s[j+1] = s[j];
            j=j-1;
        }
        s[j+1] = temp;
    }
   
    for(i=0;i<LEN; i++)
    printf("%d ",s[i]);
 
    return 0;
 }
