#include <iostream>
 
using namespace std;
 
int main()
{
    int a, b;
 
    cout << "두 개의 정수를 입력하세요: ";
    cin >> a >> b;
    if (b == 0) // 나누는 수가 0이라면,
        cout << "나누는 수가 0이 될 수 없습니다." << endl;
    else
        cout << a << "를 " << b << "로 나눈 몫은 " << a/b << "입니다." << endl;
    return 0;
}
